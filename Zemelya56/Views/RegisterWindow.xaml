<Window x:Class="Zemelya56.Views.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Zemelya56.Views"
        mc:Ignorable="d"
       Title="Register" Height="300" Width="400">
    <Grid>
        <StackPanel>
            <Label Content="Username" Margin="10,0,0,0"/>
            <TextBox Name="Username" Margin="10,10,10,0" />
            <Label Content="Password" Margin="10,10,0,0"/>
            <PasswordBox Name="Password"  Margin="10,10,10,0"/>
            <Label Content="Email" Margin="10,10,0,0"/>
            <TextBox Name="Email"  Margin="10"/>
            <Button Name="RegisterButton" Content="Register" Click="RegisterButton_Click" Margin="10"/>
        </StackPanel>
    </Grid>
</Window>
